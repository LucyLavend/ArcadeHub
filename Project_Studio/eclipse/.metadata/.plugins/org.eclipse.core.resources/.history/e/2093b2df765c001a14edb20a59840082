package game.spawnobjects.plants.big;

import nl.miystengine.client.MiystEngine;
import nl.miystengine.client.renderer.EntityRenderer;
import nl.miystengine.client.renderer.entity.LODSmallModel;
import org.lwjgl.util.vector.Vector3f;

public class BigPlantList
{
	public int spawnBigPlant(String name, Vector3f position)
	{
		if((name.contains("Giant") && name.contains("Monstera")) || (name.contains("giant") && name.contains("monstera")))
		{
			LODSmallModel small_weed = new LODSmallModel(1,"plants/monstera_big","plant/monstera_big",LODSmallModel.class);
			small_weed.materialType = 18;
			small_weed.setPosition(position);
			EntityRenderer.nmr.spawnEntity(small_weed);
		}
		else if((name.contains("Split") && name.contains("Leaf")) || (name.contains("split") && name.contains("leaf")))
		{
			LODSmallModel small_weed = new LODSmallModel(1,"plants/split_leaf","plant/monstera_big",LODSmallModel.class);
			small_weed.materialType = 18;
			small_weed.setPosition(position);
			EntityRenderer.nmr.spawnEntity(small_weed);
		}
		return -1;
	}
}