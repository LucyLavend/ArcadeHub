	public void renderWorld(float ticker)
    { 
		
		int time = (60*IngameGui.Hours)+IngameGui.minutes;
		EntityLivingBase base = this.ttw.renderViewEntity;
		
		fogDensity = base.worldObj.StructureWorldInfo.WorldFog;
		CameraAndPlayerAnimations.getUpAnimation(base);
		
		MouseControl.controlForgeObjects();
		
		PassingSeconds.seconds();
		if(PassingSeconds.secondspassed)
	    {
			if(this.skyRender!=null)
	        {
	        	this.skyRender.updateClouds();
	        }
			CameraAndPlayerAnimations.breathAnimation((EntityPlayer)base);
			
			if(Keyboard.isKeyDown(Keyboard.KEY_F1)||this.terrainChanged)
			{
				this.worldRenderer.clearTerrain();
				this.terrain = new Terrain();
        		this.worldRenderer.terrain = new RenderTerrain(); 
				this.specialRefresh=true;
				((EntityPlayer)base).hasMoved=true;
				this.updateArea();
				this.terrainChanged = false;
			}
			else if(((EntityPlayer)base).hasMoved)
			{
				if(((EntityPlayer)base).hasMoved)
				{
					CameraAndPlayerAnimations.walkAnimation((EntityPlayer)base);
				}
				else if(CameraAndPlayerAnimations.walkAnimation>0.01F)
				{
					CameraAndPlayerAnimations.walkAnimation-= 0.01F;
				}
				else if(CameraAndPlayerAnimations.walkAnimation<-0.01F)
				{
					CameraAndPlayerAnimations.walkAnimation+= 0.01F;
				}
			}
			
			MouseControl.setTime();
			if(this.waterRenderer!=null)
			{
				this.waterRenderer.updateWater();
			}
			GameProfile.AdminCommands();
			if(MouseControl.getObject==-1&&MouseControl.getWaterObject==-1&&TheStructureWorld.TheStructureWorld.canEditTerrain>0)
			{
				--TheStructureWorld.TheStructureWorld.canEditTerrain;
			}       
	    }
		
		if(((EntityPlayer)base).hasMoved && onWorldLoaded > 1)
		{
			this.updateArea();
		}
		
        if(MouseControl.changeTime)
        {
        	if(IngameGui.Hours<12)
        	{
        		sunColor = time*0.00138F;
        	}
        	else
        	{
        		sunColor=(1440-time)*0.00138F;
        	}
        }
		
        if(light.size()>0&&sunColor>0.05F)
		{
		this.light.get(0).setColour(new Vector3f(sunColor,sunColor,sunColor));
		}

		this.setFogColor(new float[]{sunColor,sunColor,sunColor});
		CameraAndPlayerAnimations.setCamposAndPitch((float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraX,(float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraY + CameraAndPlayerAnimations.walkingAnimation + CameraAndPlayerAnimations.bodyPos,(float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraZ, EntityRenderer.pitch+CameraAndPlayerAnimations.lookDownAni,CameraAndPlayerAnimations.walkAnimation*(CameraAndPlayerAnimations.drunkWalk));  
		
		this.getMouseOver(ticker);
        GL11.glClear(GL11.GL_COLOR_BUFFER_BIT | GL11.GL_DEPTH_BUFFER_BIT);
        GL11.glClearColor(this.skyColor[0], this.skyColor[1], this.skyColor[2], this.skyColor[3]);
        this.setupCameraTransform(ticker);
        if(this.serverWorld == null){this.serverWorld = TheStructureWorldServer.getServer().worldServer();}
        if((Keyboard.isKeyDown(Keyboard.KEY_F3)&&this.refershShaders==0)||this.refershShaders==111)
        {
        this.refershShaders=100;
        this.ShaderT = new TerrainShader(TerrainShader.VERTEX_FILEN,TerrainShader.FRAGMENT_FILEN);
        this.terrainRenderer = new TerrainRenderer(EntityRenderer.projectionMatrix);
        ((EntityPlayer)base).hasMoved = true;
        }
        
        if(this.refershShaders>0)
        {
        	--this.refershShaders;
        }
        
        if(onWorldLoaded == 1)
        {	
        	//this.light.add(new Light(new float[]{220,150,280},new float[]{0F,0F,1F},new float[]{0.3F,0.3F,0.3F}));
        	
        
        	RenderTerrain terrain = new RenderTerrain();
        	RenderPipes pipe = new RenderPipes();
                	
