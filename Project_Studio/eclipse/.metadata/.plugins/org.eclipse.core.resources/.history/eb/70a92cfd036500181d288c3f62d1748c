	public void renderWorld(float ticker)
    { 
		if(!(TheStructureWorld.TheStructureWorld.currentScreen instanceof GuiInventory))
		{
		if(Mouse.isButtonDown(1))
		{
		for(int id = 0;id<this.waterList.size();++id)
		{
		if(this.ttw.objectMouseOver.TerrainObjectX+2 > this.waterList.get(id).getX()  && this.ttw.objectMouseOver.TerrainObjectX-2 < this.waterList.get(id).getX())
		{
		if(this.ttw.objectMouseOver.TerrainObjectZ+2 > this.waterList.get(id).getZ()  && this.ttw.objectMouseOver.TerrainObjectZ-2 < this.waterList.get(id).getZ())
		{
		this.getWaterObject = id;
		return;
		}}		
		}
		for(int id = 0;id<BasicEntityRenderer.entityRenderList.size();++id)
		{
			if(this.ttw.objectMouseOver.TerrainObjectX+2 > BasicEntityRenderer.entityRenderList.get(id).entity.position.x  && this.ttw.objectMouseOver.TerrainObjectX-2 < BasicEntityRenderer.entityRenderList.get(id).entity.position.x)
			{
			if(this.ttw.objectMouseOver.TerrainObjectZ+2 > BasicEntityRenderer.entityRenderList.get(id).entity.position.z  && this.ttw.objectMouseOver.TerrainObjectZ-2 < BasicEntityRenderer.entityRenderList.get(id).entity.position.z)
			{
			this.getObject = id;
			return;
			}}
		}}
		else if(Mouse.isButtonDown(0))
		{
		this.getObject = -1;
		this.getWaterObject = -1;
		}}
		else if(this.getObject > 0)
		{
		this.getObject = -1;	
		this.getWaterObject = -1;
		}
		
		EntityLivingBase base = this.ttw.renderViewEntity;
		Matrix.setCamposAndPitch((float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraX,(float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraY,(float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraZ, EntityRenderer.pitch);  
		GL11.glEnable(GL11.GL_CULL_FACE);
		GL11.glEnable(GL11.GL_DEPTH_TEST);
    	this.getMouseOver(ticker);
        GL11.glViewport(0, 0, this.ttw.displayWidth, this.ttw.displayHeight);
        GL11.glClear(GL11.GL_COLOR_BUFFER_BIT | GL11.GL_DEPTH_BUFFER_BIT);
        GL11.glClearColor(skyColor[0], skyColor[1], skyColor[2], skyColor[3]);
        this.setupCameraTransform(ticker, 0);
      	GL11.glEnable(GL11.GL_ALPHA_TEST);
        GL11.glDepthRange(0, 0.001);
        if(this.serverWorld == null){this.serverWorld = TheStructureWorldServer.getServer().worldServer();}
        if(this.renderWorld2 == null){this.renderWorld2 = (WorldRenderer)this.worldRenderersToUpdate;}
        if(this.renderWorld2.terrain == null){RenderTerrain terrain = new RenderTerrain();this.renderWorld2.terrain = terrain;}
        
        
        this.mouseOverX = this.ttw.objectMouseOver.TerrainObjectX;
        this.mouseOverY = this.ttw.objectMouseOver.TerrainObjectY+Terrain.returnHeight(this.ttw.objectMouseOver.TerrainObjectX, this.ttw.objectMouseOver.TerrainObjectZ);
        this.mouseOverZ = this.ttw.objectMouseOver.TerrainObjectZ;
        
        
        if(this.onWorldLoaded <= 10)
        {
        	if(this.onWorldLoaded == 0)
            {
        	KeyAndOtherSettings.getServerPlayer();
        	this.light.add(this.sun);
        	this.farPlaneDistance = 32F;
            FOV = 110.0F;
            Aspect = (float)this.ttw.displayWidth / (float)this.ttw.displayHeight;
        	zNear = 0.05F;
        	zFar = 64F;
        	this.shader = new StaticShader();
            this.shaderW = new WaterShader();
            this.ShaderT = new TerrainShader();
         	this.skyShader = new SkyboxShader();
         	this.theRenderer = new BasicEntityRenderer();
         	this.waterRenderer = new WaterRenderer(new Loader(),this.shaderW,this.theRenderer.projectionMatrix);
         	this.terrainRenderer = new TerrainRenderer(this.ShaderT,this.theRenderer.projectionMatrix);
         	this.nmr = new NormalMappingRenderer(this.theRenderer.projectionMatrix);
            }
            this.renderWorld2.updateTerrain(base);
        	this.refreshArea4=true;
        	this.refreshArea5=true;
        	this.refreshArea6=true;
        	this.refreshArea7=true;
        	this.refreshArea8=true;
        	this.refreshArea9=true;
        	this.refreshArea10=true;
        	this.refreshArea11=true;
        	this.refreshArea12=true; 
          	++this.onWorldLoaded;
          	//terrainList.add(new Terrain((int)base.posX,154,(int)base.posZ, new Loader(), "objectsanderrain/Ground/Ground Terrain High",2,7.8F,0.08F));
          	if(this.onWorldLoaded == 5)
            {
          	terrainList.add(new Terrain(0,154,0,5,1.5625F*2));
        	this.waterList.add(WaterTile);
            }
          	String texture;	
          	if(this.random.nextInt(3)==0){texture = "palm";}
          	else if(this.random.nextInt(2)==0){texture = "leafs";}
          	else{texture = "fern";}
          	//this.theRenderer.addEntity(new BasicEntityRender(new ModelBasic(fern,"/objectsanderrain/"+texture,0.4F,0, false,true), new float[]{this.random.nextInt(25),154F,this.random.nextInt(25)},0,0,0,0,scale));
          	float X = this.random.nextInt(28);
          	float Z = this.random.nextInt(28);
          	String XZ = ""+(X*Z);
          	if(XZ.length()>3){XZ = XZ.substring(XZ.length()-(XZ.length()-1));}
          	float FixScale = ((Float.parseFloat(XZ)/XZ.length())/(300F));         	
          	if(FixScale>0.3F){FixScale-=(FixScale-0.3);}  
          	if(FixScale<0.1F){FixScale+=(0.1-FixScale);} 
          	this.theRenderer.addEntity(new BasicEntityRender(new ModelBasic(boulder,"/objectsanderrain/boulder",0.4F,1,false,true), new float[]{X,154F+Terrain.returnHeight(X*0.142857F,Z*0.142857F),Z},0,this.random.nextInt(360),this.random.nextInt(360),this.random.nextInt(360),FixScale));
        }  
        
        GameProfile.AdminCommands();
        

        if(this.refreshArea4){this.renderWorld2.renderAreaTerrain(4,this.renderWorld2.ArrayListPlant4,this.renderWorld2.ArrayListStone4,this.renderWorld2.ArrayListTerrain4,base);this.refreshArea4=false;}
        if(this.refreshArea5){this.renderWorld2.renderAreaTerrain(5,this.renderWorld2.ArrayListPlant5,this.renderWorld2.ArrayListStone5,this.renderWorld2.ArrayListTerrain5,base);this.refreshArea5=false;}
        if(this.refreshArea6){this.renderWorld2.renderAreaTerrain(6,this.renderWorld2.ArrayListPlant6,this.renderWorld2.ArrayListStone6,this.renderWorld2.ArrayListTerrain6,base);this.refreshArea6=false;}
        if(this.refreshArea7){this.renderWorld2.renderAreaTerrain(7,this.renderWorld2.ArrayListPlant7,this.renderWorld2.ArrayListStone7,this.renderWorld2.ArrayListTerrain7,base);this.refreshArea7=false;}
        if(this.refreshArea8){this.renderWorld2.renderAreaTerrain(8,this.renderWorld2.ArrayListPlant8,this.renderWorld2.ArrayListStone8,this.renderWorld2.ArrayListTerrain8,base);this.refreshArea8=false;}
        if(this.refreshArea9){this.renderWorld2.renderAreaTerrain(9,this.renderWorld2.ArrayListPlant9,this.renderWorld2.ArrayListStone9,this.renderWorld2.ArrayListTerrain9,base);this.refreshArea9=false;}
        if(this.refreshArea10){this.renderWorld2.renderAreaTerrain(10,this.renderWorld2.ArrayListPlant10,this.renderWorld2.ArrayListStone10,this.renderWorld2.ArrayListTerrain10,base);this.refreshArea10=false;}
        if(this.refreshArea11){this.renderWorld2.renderAreaTerrain(11,this.renderWorld2.ArrayListPlant11,this.renderWorld2.ArrayListStone11,this.renderWorld2.ArrayListTerrain11,base);this.refreshArea11=false;}
        if(this.refreshArea12){this.renderWorld2.renderAreaTerrain(12,this.renderWorld2.ArrayListPlant12,this.renderWorld2.ArrayListStone12,this.renderWorld2.ArrayListTerrain12,base);this.refreshArea12=false;}
        this.renderWorld2.renderTestObject(base, 2);
        if(this.renderWorld2 != null)
        {
        	this.allRenderLists.setupRenderList(this.renderWorld2.posXMinus, this.renderWorld2.posYMinus, this.renderWorld2.posZMinus, base.lastTickPosX + (base.posX - base.lastTickPosX) * ticker, base.lastTickPosY + (base.posY - base.lastTickPosY) * ticker, base.lastTickPosZ + (base.posZ - base.lastTickPosZ) * ticker);
        }
        GL11.glTranslatef((float)(this.allRenderLists.renderAreaX - this.allRenderLists.cameraX), (float)(this.allRenderLists.renderAreaY - this.allRenderLists.cameraY), (float)(this.allRenderLists.renderAreaZ - this.allRenderLists.cameraZ));
        if(((EntityPlayer)base).hasMoved)
        {
    		this.updateArea();
        }
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////       
        int seconds = (int)((float)TheStructureWorld.TheStructureWorld.thePlayer.worldObj.getWorldTime()/100);
        int hours = (((seconds/60) - (((seconds/60)/24)*24))/2)*17;
        GL11.glEnable(GL11.GL_TEXTURE_2D);
    	GL11.glTexParameteri(GL11.GL_TEXTURE_2D, GL11.GL_TEXTURE_WRAP_S, GL12.GL_CLAMP_TO_EDGE);
    	GL11.glTexParameteri(GL11.GL_TEXTURE_2D, GL11.GL_TEXTURE_WRAP_T, GL12.GL_CLAMP_TO_EDGE);
        GL11.glPushMatrix();
        float x = (float)((this.allRenderLists.renderAreaX - this.allRenderLists.cameraX));
        float y = (float)((this.allRenderLists.renderAreaY - this.allRenderLists.cameraY));
        float z = (float)((this.allRenderLists.renderAreaZ - this.allRenderLists.cameraZ));
        this.renderWorld2.plants.randomGrassNummer();
        if(new String(""+this.renderWorld2.plants.Movement1).contains("0.0")){this.WindStrengh = base.worldObj.WindStrengh;}
        for(int I=0;I<this.WindStrengh;++I){this.renderWorld2.plants.SwingAnimation1();this.renderWorld2.plants.SwingAnimation2();this.renderWorld2.plants.SwingAnimation3();this.renderWorld2.plants.SwingAnimation4();}
        GL11.glPopMatrix();
        this.theRenderer.animatePlant(2, (float)this.renderWorld2.plants.LeftOrRight1, -1, (float)this.renderWorld2.plants.LeftOrRight3);
        this.theRenderer.renderBasicEntity(light,4 ,null,10,true); 
        this.terrainRenderer.render(terrainList,light,null,10,true);
        this.waterRenderer.render(this.waterList);
     }
	
	public static float waterHeight = 155F;
	
	public void reflection()
	{
		if(this.theRenderer != null)
		{
		float distance = (2*((float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraY-waterHeight));        	
		buffer.bindReflectionFrameBuffer();
	    GL11.glClear(GL11.GL_COLOR_BUFFER_BIT | GL11.GL_DEPTH_BUFFER_BIT);
	    Matrix.pitch();
	    Matrix.setCamY((float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraY-distance);
	    //GL11.glDisable(GL11.GL_CULL_FACE);
	    this.theRenderer.renderBasicEntity(light,4 ,new Vector4f(0,1,0,-waterHeight),1,true);
	    GL11.glDisable(GL11.GL_CULL_FACE);
	    this.terrainRenderer.render(terrainList,light,new Vector4f(0,1,0,-waterHeight),1,true);
	    buffer.unbindCurrentFrameBuffer();
	    Matrix.setCamY((float)nl.thestructureworld.client.renderer.EntityRenderer.allRenderLists.cameraY);
	    Matrix.pitch();
		}
	}
	 
	public void refraction()
	{
		if(this.theRenderer != null)
		{
		buffer.bindRefractionFrameBuffer();
	    GL11.glClear(GL11.GL_COLOR_BUFFER_BIT | GL11.GL_DEPTH_BUFFER_BIT);
	    this.theRenderer.renderBasicEntity(light,4 ,new Vector4f(0,-1,0,waterHeight),1,false);
	    this.terrainRenderer.render(terrainList,light,new Vector4f(0,-1,0,waterHeight),1,false);
        buffer.unbindCurrentFrameBuffer();
		}
	}
	
