        GL11.glEnable(GL11.GL_FOG);
        GL11.glDepthRange(0, 0.01);
        //this.renderEntities(ticker); 


////////////////////////////////////////////////////////////////////////////////////////////////////////
        GL11.glTranslated(0, 0, 0);
        if(this.renderWorld2.objectRemoved || this.onWorldEntered <= 5)
        {
        	RenderTerrain terrain = new RenderTerrain();
        	this.renderWorld2 = (WorldRenderer)this.worldRenderersToUpdate;
        	this.renderWorld2.terrain = terrain;
        	this.renderWorld2.updateTerrain(base);
        	EntityLivingBase RenderVieuw = TheStructureWorld.getTheStructureWorld().renderViewEntity;
        	this.renderWorld2.renderDirtTerrain(base, 2);
        	this.renderWorld2.renderPlant(base, 3);
        	WorldRenderer.objectRemoved = false;
        	this.lightmapUpdateNeeded = true;
        	++this.onWorldEntered;
        }
        this.renderWorld2.renderTestObject(base, 5);

        if(this.serverWorld != null && RenderTerrain.serverWorld == null)
        {
        	IngameMenuAndOptionsGui options = new IngameMenuAndOptionsGui();
            options.loadorwriteOptions(true);
            RenderTerrain.serverWorld = this.serverWorld;
        }
        if(((EntityPlayer)base).hasMoved)
        {
        	this.renderWorld2.renderSky(base,1);
        }
        if(this.renderWorld2 != null)
        {
        this.glRenderLists.clear();
        this.glRenderLists.add(this.renderWorld2);
        this.allRenderLists.resetList();
        this.allRenderLists.setupRenderList(this.renderWorld2.posXMinus, this.renderWorld2.posYMinus, this.renderWorld2.posZMinus, base.lastTickPosX + (base.posX - base.lastTickPosX) * ticker, base.lastTickPosY + (base.posY - base.lastTickPosY) * ticker, base.lastTickPosZ + (base.posZ - base.lastTickPosZ) * ticker);
        this.allRenderLists.addGLRenderList(new int[]{1,2,3,5});
        }
               
        this.ttw.entityRenderer.enableLightmap(ticker);
        GL13.glMultiTexCoord2f(33985, (float)5000 % 65536 / 1.0F, (float)5000 / 65536 / 1.0F);
        TheStructureWorld.TheStructureWorld.getTextureManager().bindTexture("/objectsanderrain/sky.png", true); 
        this.allRenderLists.callList(1);
        TheStructureWorld.TheStructureWorld.getTextureManager().bindTexture("/objectsanderrain/Ground Terrain "+IngameMenuAndOptionsGui.basicTerrain+".png", true); 
        this.allRenderLists.callList(2);
        TheStructureWorld.TheStructureWorld.getTextureManager().bindTexture("/objectsanderrain/Plants Normal.png", true);
        this.allRenderLists.callList(3);
        TheStructureWorld.TheStructureWorld.getTextureManager().bindTexture(KeyAndOtherSettings.pictureFile, true);
        this.allRenderLists.callList(5);
        this.renderWorld2.terrain.randomGrassNummer();
        
        if(new String(""+this.renderWorld2.terrain.Movement1).contains("0.0"))
        {
        this.WindStrengh = base.worldObj.WindStrengh;
        }
        for(int I=0;I<this.WindStrengh;++I){this.renderWorld2.terrain.cornSwingAnimation();this.renderWorld2.terrain.grassSwingAnimation();}
        
        this.renderWorld2.renderGrass(base,this.allRenderLists.renderAreaX,this.allRenderLists.renderAreaY,this.allRenderLists.renderAreaZ,this.allRenderLists.cameraX,this.allRenderLists.cameraY,this.allRenderLists.cameraZ);
       
       
        this.ttw.entityRenderer.disableLightmap(ticker);
